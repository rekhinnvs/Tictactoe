<Window x:Class="TicTacToe.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
        Background="{DynamicResource MaterialDesignPaper}"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        xmlns:local="clr-namespace:TicTacToe"
        mc:Ignorable="d"
        Title="GameWindow" Height="600" Width="500" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>

        <Canvas x:Name="CanvasGamePage" Width="500" Height="500" d:IsHidden="True" Grid.ColumnSpan="2" Margin="0,35,-6.4,35.4"/>
        <Canvas x:Name="CanvasFinishPage" Width="500" Height="500" d:IsHidden="True" Grid.ColumnSpan="2" Margin="0,35,-6.4,35.4"/>
        <Label Content="Label" Name="lbltest" HorizontalAlignment="Left" Margin="190,150,0,0" VerticalAlignment="Top" Height="40" Width="170"/>
    </Grid>
</Window>
